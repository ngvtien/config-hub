{
  "name": "config-hub",
  "version": "1.0.0",
  "description": "Electron app with React, TypeScript, Vite, Tailwind CSS, and shadcn/ui",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite --host",
    "dev:electron": "vite",
    "electron:start": "wait-on tcp:5173 && electron .",
    "build": "npm run type-check && vite build && electron-builder --config electron-builder-simple.json",
    "build:fast": "npm run type-check && vite build && electron-builder --config electron-builder-simple.json",
    "build:web": "npm run type-check && vite build",
    "build:dir": "npm run type-check && vite build && electron-builder --dir --config electron-builder-optimized.json",
    "build:skip-types": "vite build && electron-builder --config electron-builder-optimized.json",
    "build:dev": "vite build",
    "build:electron": "electron-builder --config electron-builder-optimized.json",
    "preview": "vite preview",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "node tests/credentials/test-credential-core.js",
    "test:credentials": "node tests/credentials/test-credential-core.js",
    "test:all": "node tests/credentials/test-credential-core.js && node tests/credentials/test-simple-credentials.js"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "vite",
    "tailwind"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.19",
    "@types/node-forge": "^1.3.14",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.19.0",
    "@codemirror/commands": "^6.9.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.4.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.3",
    "@codemirror/legacy-modes": "^6.5.2",
    "@codemirror/lint": "^6.9.0",
    "@codemirror/merge": "^6.11.0",
    "@codemirror/search": "^6.5.11",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rjsf/core": "^5.24.13",
    "@rjsf/utils": "^5.24.13",
    "@rjsf/validator-ajv8": "^5.24.13",
    "@uiw/codemirror-theme-atomone": "^4.25.2",
    "@uiw/codemirror-theme-github": "^4.25.2",
    "@uiw/codemirror-theme-vscode": "^4.25.2",
    "@uiw/react-codemirror": "^4.25.2",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "codemirror": "^6.0.2",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "lucide-react": "^0.376.0",
    "monaco-yaml": "^5.4.0",
    "node-forge": "^1.3.1",
    "react": "^18.2.0",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "remark-gfm": "^4.0.1",
    "simple-git": "^3.28.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.8"
  }
}
