{
  "appId": "com.example.electron-react-app",
  "productName": "Electron React App",
  "directories": {
    "output": "release/${version}"
  },
  "files": [
    "dist",
    "dist-electron"
  ],
  "win": {
    "target": [
      {
        "target": "portable",
        "arch": [
          "x64"
        ]
      }
    ],
    "artifactName": "${productName}_${version}.${ext}"
  },
  "mac": {
    "artifactName": "${productName}_${version}.${ext}",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "target": [
      "AppImage"
    ],
    "artifactName": "${productName}_${version}.${ext}"
  }
}